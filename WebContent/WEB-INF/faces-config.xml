<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">
	
	<navigation-rule>
		<from-view-id>/registroP.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>registrar</from-outcome>
			<to-view-id>/registroExitoso.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/selectItem.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>item</from-outcome>
			<to-view-id>/item.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>
	
</faces-config>